<% include ../partials/header %>

    <section id="gallery" class="gallery">
        <h3>Gallery</h3>
        <div class="gallery-wrapper">
        <% products.forEach(function(painting) { %>
            <div class="gallery-item">
                <img src="<%= painting.imagePath %>" alt="<%= painting.title %> painting" id="<%= painting._id %>">
            </div>
        <% }) %>
       
        </div>
    </section>
 
 <% include ../partials/footer %>